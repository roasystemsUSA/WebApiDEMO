{
  "ConnectionStrings": {
    "DefaultConnection": "[Put here your Default connection]",
    "QAConnection": "[Put here your QA connection]",
    "ProductionConnection": "[Put here your Production connection]"
  },
  "Environments": [
    {
      "Name": "Development",
      "ConnectionString": "DefaultConnection"
    },
    {
      "Name": "QA",
      "ConnectionString": "QAConnection"
    },
    {
      "Name": "Production",
      "ConnectionString": "ProductionConnection"
    }
  ],
  "Environment": "Production",
  "Authentication": {
    "Google": {
      "ClientId": "[Put Here your Google Client ID]",
      "ClientSecret": "[Put here your Google Client Secret]"
    }
  },
  "EmailSettings": {
    "SmtpServer": "smtp.gmail.com",
    "SmtpPort": 587,
    "SmtpUser": "[Put here your Google account Id]",
    "SmtpPassword": "[Put here your Google password]"
  },
  "Jwt": {
    "Issuer": "[Put here your JWT Issuer]",
    "Audience": "[Put here your JWT Aucience]",
    "SecretKey": "[Put here your JWT Secret key (55 chars lenght at least)]",
    "ExpiresInMinutes": 2880
  },
  "ASPSMS": {
    "UserKey": "[Put here your ASPSMS UserKey]",
    "Password": "[Put here your ASPSMS Password]",
    "Originator": "[Put here your ASPSMS originator]"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}